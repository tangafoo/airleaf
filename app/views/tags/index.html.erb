<%@listing.tags.each do |extra_tag|%>
<%if default_tags.include?(extra_tag.tag)%>
<%else%>
<p><%=extra_tag.tag%> <%=link_to 'Edit', edit_extra_tag_path(@listing.id, extra_tag.id)%>
  <%=link_to 'Delete', delete_extra_tag_path(@listing.id, extra_tag.id), method: :delete, data: {confirm: "Are you sure?"}%></p>
<%end%>
<%end%>
